package Programs;

import java.util.Scanner;

public class prime {

	public static void main(String[] args) {
		int n,count=0;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the value:");
		n = sc.nextInt();
		
		for(int i=2;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				if(i%j==0) {
					count++;
				}
			}
			if(count==2) {
				System.out.println(i+" ");
			}
		}
		
	}

}
