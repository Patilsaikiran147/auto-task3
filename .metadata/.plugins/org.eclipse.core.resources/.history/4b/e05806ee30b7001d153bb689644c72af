package Programs;

import java.util.Scanner;

public class Swap1 {
	
	public static void hello(int a, int b) {
		a = a + b;
		b = a - b;
		a = a - b;
		System.out.println(a + " " + b);
	}

	public static void main(String[] args) {
		System.out.println("enter the values:");
		Scanner sc = new Scanner(System.in);
		int nxt = sc.nextInt();
		int nex1 = sc.nextInt();
		hello(nxt,nex1);
		
	}

}
