package Programs;

import java.util.Scanner;

public class Swap1 {
	static int a,b;
		static void hello() {
			a=a+b;
			b=a-b;
			a=a-b;
			
		}
	public static void main(String[] args) {
System.out.println("enter the values:");
Scanner sc = new Scanner(System.in);
int nxt = sc.nextInt();
hello();
System.out.println(a+" "+b);
	}

}
